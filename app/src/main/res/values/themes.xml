<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Base.Theme.AnNet" parent="Theme.Material3.DayNight.NoActionBar">
        <item name="fontFamily">@font/poppins_regular</item>
        <item name="windowSplashScreenAnimatedIcon">@drawable/icon</item>
        <item name="windowSplashScreenAnimationDuration">1000</item>
        <item name="android:statusBarColor">@color/background</item>
        <item name="android:windowLightStatusBar">true</item>
    </style>

    <style name="BottomAppBarTheme" parent="Widget.Material3.BottomNavigationView">
        <item name="materialThemeOverlay">@style/BottomAppBarThemeOverlay</item>
    </style>

    <style name="BottomAppBarThemeOverlay" parent="">
        <item name="colorSecondaryContainer">@color/primary_alpha50</item>
        <item name="colorOnSecondaryContainer">@color/white</item>
        <item name="colorOnSurface">@color/black</item>
    </style>

    <style name="Theme.AnNet" parent="Base.Theme.AnNet" />
</resources>